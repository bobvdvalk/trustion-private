{
  "name": "directus-extension-trustion",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "directus:extension": {
    "host": "^10.1.3",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "demo",
        "source": "src/controllers/index.ts"
      }
    ]
  },
  "description": "Trustion Platform",
  "icon": "extension",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "scripts": {
    "dev": "directus-extension build -w --no-minify",
    "build": "directus-extension build"
  },
  "dependencies": {
    "models": "*"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.3",
    "@types/node": "^20.2.5",
    "typescript": "^5.1.3"
  }
}
